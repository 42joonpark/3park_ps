#include <string>
#include <vector>

using namespace std;

int solution(int n) {
    int answer = 0;
    vector<int> arr(n + 1);
    for (int i = 2; i * i <= n; ++i) {
        for (int k = 2; i * k <= n; ++k) {
            arr[i * k]++;
        }
    }
    for (int i = 2; i <= n; ++i) {
        if (arr[i] == 0) {
            answer++;
        }
    }
    return answer;
}
