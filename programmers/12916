#include <string>
#include <iostream>
using namespace std;

bool solution(string s)
{
    int p = 0;
    int y = 0;
    for (auto it = s.begin(); it != s.end(); ++it) {
        if (*it == 'p' || *it == 'P')
            ++p;
        if (*it == 'y' || *it == 'Y')
            ++y;
    }
    return p == y;
}
