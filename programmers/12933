#include <string>
#include <vector>

using namespace std;

long long solution(long long n) {
    long long answer = 0;
   	vector<int> arr(10, 0);
    while (n > 0) {
        arr[n % 10]++;
        n /= 10;
    }
    for (int i = 9; i >= 0; --i) {
        for (int k = 0; k < arr[i]; ++k) {
            answer *= 10;
            answer += i;
        }
    }
    return answer;
}
