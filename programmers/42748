#include <string>
#include <vector>
#include <algorithm>

using namespace std;

vector<int> solution(vector<int> array, vector<vector<int>> commands) {
    vector<int> answer;
    for (auto vec : commands) {
        vector<int> a(array.begin() + vec[0] - 1, array.begin() + vec[1]);
        sort(a.begin(), a.end());
        answer.push_back(a[vec[2] - 1]);
    }
    return answer;
}
