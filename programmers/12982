#include <iostream>
#include <stdio.h>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int arr[100001];

int solution(vector<int> d, int budget) {
    int answer = 0;
    for (int i = 0; i < d.size(); ++i)
        arr[d[i]]++;
    for (int i = 1; i < 100001; ++i) {
        for (int k = 0; k < arr[i]; ++k) {
            if (budget - i < 0) return answer;
            answer++;
            budget -= i;
        }
    }
}
