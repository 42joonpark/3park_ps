#include <string>
#include <vector>

using namespace std;

vector<int> solution(vector<int> lottos, vector<int> win_nums) {
    vector<int> answer(2, 0);
    int ct = 0;
    int zero = 0;
    for (auto a : win_nums) {
        for (int i = 0; i < lottos.size(); ++i) {
            if (a == lottos[i])
                ct++;
            if (lottos[i] == 0)
                zero++;
        }
    }
    zero /= 6;
    if (ct == 1 || ct == 0)
        answer[1] = 6;
    else if (ct == 2)
        answer[1] = 5;
    else if (ct == 3)
        answer[1] = 4;
    else if (ct == 4)
        answer[1] = 3;
    else if (ct == 5)
        answer[1] = 2;
    else answer[1] = 1;
    
    if (ct + zero == 1 || ct + zero == 0)
        answer[0] = 6;
    else if (ct + zero == 2)
        answer[0] = 5;
    else if (ct + zero == 3)
        answer[0] = 4;
    else if (ct + zero == 4)
        answer[0] = 3;
    else if (ct + zero == 5)
        answer[0] = 2;
    else answer[0] = 1;
    return answer;
}
