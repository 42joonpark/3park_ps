#include <string>
#include <vector>

using namespace std;

int solution(string s) {
    int answer = 0;
    int flag = 1;
    for (auto a = s.begin(); a != s.end(); ++a) {
        if (isdigit(*a)) {
            answer *= 10;
            answer += *a - '0';
        }
        else if (*a == '-')
            flag = -1;
    }
    return answer * flag;
}
